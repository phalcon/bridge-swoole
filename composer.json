{
    "name": "phalcon/bridge-swoole",
    "description": "Bridge to run Phalcon with Swoole.",
    "keywords": [
        "extension",
        "phalcon",
        "swoole",
        "bridge"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Phalcon Team",
            "email": "team@phalcon.io",
            "homepage": "https://phalcon.io/en/team"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/phalcon/bridge-swoole/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-phalcon": "^5.1",
        "ext-swoole": "^5.0"
    },
    "require-dev": {
        "phalcon/ide-stubs": "^5.1",
        "swoole/ide-helper": "^5.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Phalcon\\Bridge\\Swoole\\": "src/"
        }
    },
    "support": {
        "email": "support@phalcon.io",
        "issues": "https://github.com/phalcon/cphalcon/issues",
        "forum": "https://phalcon.io/forum/",
        "source": "https://github.com/phalcon/cphalcon",
        "docs": "https://phalcon.io/docs/",
        "rss": "https://blog.phalcon.io/rss"
    }
}
